package com.minnano.model;

import java.sql.Timestamp;

public class BoardBean {
	
	private int eNum;			//프라이머리 키
	private int ebNum;			//책 번호 가져오는거
	private String eSubject;	//게시물 제목
	private String eContent;	//내용
	private int eLike;			//좋아요
	private int eunLike;		//싫어요
	private String eId;			//누가 썼는지
	private int eViews;			//조회수
	private Timestamp eReg_date = null;
	private int eAvailable;		//게시판이 존재하는지 여부
	

	public int geteNum() {
		return eNum;
	}
	public void seteNum(int eNum) {
		this.eNum = eNum;
	}
	public int getEbNum() {
		return ebNum;
	}
	public void setEbNum(int ebNum) {
		this.ebNum = ebNum;
	}
	public String geteSubject() {
		return eSubject;
	}
	public void seteSubject(String eSubject) {
		this.eSubject = eSubject;
	}
	public String geteContent() {
		return eContent;
	}
	public void seteContent(String eContent) {
		this.eContent = eContent;
	}
	public int geteLike() {
		return eLike;
	}
	public void seteLike(int eLike) {
		this.eLike = eLike;
	}
	public int getEunLike() {
		return eunLike;
	}
	public void setEunLike(int eunLike) {
		this.eunLike = eunLike;
	}
	public String geteId() {
		return eId;
	}
	public void seteId(String eId) {
		this.eId = eId;
	}
	public int geteViews() {
		return eViews;
	}
	public void seteViews(int eViews) {
		this.eViews = eViews;
	}
	
	public Timestamp geteReg_date() {
		return eReg_date;
	}
	public void seteReg_date(Timestamp ereg_date) {
		this.eReg_date = ereg_date;
	}
	
	public int geteAvailable() {
		return eAvailable;
	}
	public void seteAvailable(int eAvailable) {
		this.eAvailable = eAvailable;
	}
	
	@Override
	public String toString() {
		return "BoardBean [eNum=" + eNum + ", ebNum=" + ebNum + ", eSubject=" + eSubject + ", eContent=" + eContent
				+ ", eLike=" + eLike + ", eunLike=" + eunLike + ", eId=" + eId + ", eViews=" + eViews + ", eReg_date="
				+ eReg_date + ", eAvailable=" + eAvailable + "]";
	}

	
	
	
	
}
